{
	"name": "1a_pl_dim_sql_to_azure_full_load",
	"properties": {
		"activities": [
			{
				"name": "Lookup_config_tbl",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
